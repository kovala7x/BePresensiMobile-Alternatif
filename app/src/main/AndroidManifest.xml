<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trateg.bepresensimobile">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:launchMode="singleTop"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.lihat_presensi_mahasiswa.LihatPresensiMahasiswaActivity"></activity>
        <activity android:name=".ui.ubah_status_presensi.UbahStatusPresensiActivity" />
        <activity android:name=".ui.detail_presensi_kelas.DetailPresensiKelasActivity" />
        <activity android:name=".ui.lihat_presensi_kelas.LihatPresensiKelasActivity" />
        <activity android:name=".ui.detail_berita_acara.DetailBeritaAcaraActivity" />
        <activity android:name=".ui.isi_berita_acara.IsiBeritaAcaraActivity" />
        <activity android:name=".ui.otentikasi_presensi.OtentikasiPresensiActivity" />
        <activity android:name=".ui.atur_toleransi.AturToleransiActivity" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:theme="@style/LoginTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.ajukan_surat.AjukanSuratActivity" />
        <activity android:name=".ui.main.MainActivity" />
    </application>

</manifest>